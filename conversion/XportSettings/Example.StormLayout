  <Constant name="TopBarShowBasicAbilities" val="true" /> <!-- When set to false, Basic Abilities will not be shown in the custom Top Bar -->
  <Constant name="TopBarShowVehiclePassengerHealth" val="true" /> <!-- When set to false, Health of passengers of Vehicles will not be shown in the custom Top Bar -->
