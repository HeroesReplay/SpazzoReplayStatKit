  <Constant name="MinimapVisibilityToggle" val="false" /> <!-- When set to true, don't display the Minimap -->
  <Constant name="HideAttachmentOnCD" val="false" /> <!-- When set to true, Hero Attachments using Abilities will not display while that ability is on cooldown -->
  <Constant name="UIShortcutVisibilityToggle" val="true" /> <!-- When set to true, the button to open the options menu will not be displayed -->
  <Constant name="TopBarShowNonQuests" val="true" /> <!-- When set to true, non-quests will be shown(in a different font & border) in the custom Top Bar -->
  <Constant name="TopBarDisplayTooltips" val="true" /> <!-- When set to true, tooltips will be enabled in the custom Top Bar -->
  <Constant name="TopBarShowTalents" val="false" /> <!-- When set to false, Talents other than the Heroic will not be displayed in the custom Top Bar -->
